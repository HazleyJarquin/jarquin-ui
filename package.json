    "react-native": ">=0.64.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-svg": "15.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.22.15",
    "@react-native-async-storage/async-storage": "^2.0.0",
